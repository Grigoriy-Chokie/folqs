                    {%- capture input_id -%}
                      {{ section.id }}-{{ forloop.index0 -}}
                    {%- endcapture -%}

                    {% assign compare_at_price_difference = price_compare
                      | divided_by: 100.0
                      | times: plan.selling_plans.first.price_adjustments.first.value
                    %}

                    <input
                      type="radio"
                      id="{{ input_id }}"
                      name="{{ plan.name }}"
