                        <input
                          class="buy-quantity__input quantity__input gift-quantity__input visually-hidden"
                          type="number"
                          name="items[1][quantity]"
                          min="1"
                          id="Quantity-{{ section.id }}"
                          value="{{ product.selected_or_first_available_variant.quantity_rule.min }}"
                          form="{{ product_form_id }}"
                        >
                      {% endif %}
                      {% if buy_buttons_block.settings.child_product and buy_buttons_block.settings.child_product != blank %}
                        <input
                          class="buy-quantity__input quantity__input gift-quantity__input visually-hidden"
                          type="number"
                          name="items[2][quantity]"
                          min="1"
                          id="Quantity-{{ section.id }}"
                          value="{{ buy_buttons_block.settings.child_product.selected_or_first_available_variant.quantity_rule.min }}"
                          form="{{ product_form_id }}"
                        >
                      {% endif %}
                      
                    </label>
                    <button
                      class="buy-quantity__button quantity__button buy-quantity__button-plus no-js-hidden"
                      name="plus"
                      type="button"
                    >
                      {%- render 'buy-buttons-icon-plus' -%}
                    </button>
                  </quantity-input>
                {%- endif -%}
                {%- render 'buy-buttons',
                  gift_product: block.settings.gift_product
                  block: block,
                  product: product,
                  product_form_id: product_form_id,
                  section_id: section.id,
                  show_bynamic_button: false,
                  additional_bottom_text: block.settings.bottom_text
                -%}
                {% liquid
                  assign is_show_benefits = false
                  for benefit_index in (1..3)
                    assign benefit_id = 'benefits_text' | append: benefit_index
                    if block.settings[benefit_id] != blank
                      assign is_show_benefits = true
                      break
                    endif
                  endfor
                %}
                {% if is_show_benefits == true and block.settings.icon != blank %}
                  <div class="buy-buttons__benefits">
                    {% for benefit_index in (1..3) %}
                      {% assign benefit_id = 'benefits_text' | append: benefit_index %}
                      {% if block.settings[benefit_id] != blank %}
                        <div class="buy-buttons__benefit">
                          {{
                            block.settings.icon
                            | image_url: width: master
                            | image_tag: widths: '100', alt: block.settings.icon.alt, loading: 'lazy'
                          }}
                          <span>
                            {{ block.settings[benefit_id] }}
                          </span>
                        </div>
                      {% endif %}
                    {% endfor %}
                  </div>
                {% endif %}
              </div>
            {%- when 'payment_icons' -%}
              <div class="product-information__payment-icons">
                {% for i in (1..10) %}
                  {%- assign icon_key = 'image_' | append: i -%}
                  {% if block.settings[icon_key] != blank %}
                    {{ block.settings[icon_key] | image_url: width: 62 | image_tag }}
                  {% endif %}
                {% endfor %}
              </div>
            {%- when 'guarantee' -%}
              <div class="product-content__guarantee">
                <div class="guarantee__header">
                  {%- unless block.settings.icon == blank -%}
                    <div class="guarantee__icon">
                      {{ block.settings.icon | image_url: width: 40 | image_tag }}
                    </div>
                  {%- endunless -%}
                  {%- unless block.settings.title == blank -%}
                    <p class="guarantee__title">{{ block.settings.title }}</p>
                  {%- endunless -%}
                </div>
                {%- unless block.settings.text == blank -%}
                  <p class="guarantee__text">
                    {{ block.settings.text }}
                  </p>
                {%- endunless -%}
              </div>
            {%- when 'review_with_usp' -%}
              <div class="product-content__reviews-with-usp product-content__reviews-with-usp--{{ block.settings.style }}">
                <div class="reviews-with-usp__content">
                  <div class="reviews-with-usp__stars">
                    {% liquid
                      assign tag = 'div'
                      if block.settings.stars_url != blank
                        assign tag = 'a'
                      endif
                    %}

                    {%- if block.settings.image != blank -%}
                      <div class="reviews-with-usp__image reviews-with-usp__image--mobile-only">
                        {{- block.settings.image | image_url: width: 224 | image_tag -}}
                      </div>
                    {%- endif -%}
                    <{{ tag }} href="{{ block.settings.stars_url }}" class="rating__stars">
                      {{ 'icon-reviews-stars-5.svg' | inline_asset_content }}
                    </{{ tag }}>
                  </div>
                  {%- if block.settings.image != blank and block.settings.extra_styles -%}
                    <div class="reviews-with-usp__image">
                      {{- block.settings.image | image_url: width: 224 | image_tag -}}
                    </div>
                  {%- endif -%}
                  <p class="reviews-with-usp__title">{{ block.settings.title }}</p>
                  {% if section.settings.hide_usp_icons != true %}
                    <div class="product-content__usp-icons reviews-with-usp__icons">
                      <div class="usp-icons__info-item info-item--shipping">
                        {{ 'icon-usp-deal-shipping.svg' | inline_asset_content }}
                        <span>{{ 'products.product.landing.usp_icons.shipping_html' | t | strip_html }}</span>
                      </div>
                      <div class="usp-icons__info-item info-item--secure">
                        {{ 'icon-usp-secure-black.svg' | inline_asset_content }}
                        <span>{{ 'products.product.landing.usp_icons.secure_html' | t | strip_html }}</span>
                      </div>
                      <div class="usp-icons__info-item info-item--deals">
                        {{ 'icon-usp-usa-black.svg' | inline_asset_content }}
                        <span>{{ 'products.product.landing.usp_icons.usa_html' | t | strip_html }}</span>
                      </div>
                    </div>
                  {% endif %}
                </div>
                {%- if block.settings.image != blank and block.settings.extra_styles == false -%}
                  <div class="reviews-with-usp__image reviews-with-usp__image--desktop-only">
                    {{- block.settings.image | image_url: width: 224 | image_tag -}}
                  </div>
                {%- endif -%}
              </div>
          {%- endcase -%}
        {% endfor %}
        {% if icon_text_block_count > 0 %}
          <div class="icon-text-blocks">
            {% for block in section.blocks %}
              {% if block.type == 'icon-text' and block.settings.text != blank %}
                <div class="icon-text-block">
                  {% if block.settings.icon != blank %}
                    <img src="{{ block.settings.icon | image_url: width: 32, height: 32 }}" alt="{{ block.settings.icon.alt }}" width="32" height="32" loading="lazy">
                  {% endif %}
                  <span>{{ block.settings.text | newline_to_br }}</span>
                </div>
              {% endif %}
            {% endfor %}
          </div>
        {% endif %}
        
        {% if collapsible_row_blocks.size > 0 %}
          <div class="faq__content faq__content--{{ section.settings.faq_style }}">
            {% for block in collapsible_row_blocks %}
              {%- liquid
                assign question = block.settings.question
                assign answer = block.settings.answer
                assign class_name_content = 'faq-item__answer'

                if block.settings.default_list
                  assign class_name_content = class_name_content | append: ' faq-item__answer--default-list'
                endif

                if question != blank and answer != blank
                  assign has_content = true
                else
                  assign has_content = false
                endif

                if has_content == true
                  render 'collapse-row', icon_style: 'new', title: question, content: answer, class_name_row: 'faq__content-item faq-item', class_name_title: 'faq-item__question', class_name_content: class_name_content, is_first_opened: is_first_opened, index: forloop.index0
                endif
              -%}
            {% endfor %}
          </div>
        {% endif %}
      </div>
    </div>

    <script type="application/json" class="landing-product-klaviyo-data">
      {
        "productItem": {
          "ProductName": "{{ product.title }}",
          "ProductID": "{{ product.id }}",
          "SKU": "{{ product.product.selected_or_first_available_variant.sku }}",
          "ImageURL": "{{ product.featured_image }}",
          "URL": "{{ product.url }}",
          "Brand": "{{ product.vendor }}",
          "Price": {{ product.price }}
        },
        "companyId": "{{ settings.klaviyo_id }}"
      }
    </script>
  </div>

  {%- assign sticky_add_block = section.blocks | find: 'type', 'sticky-add' -%}
