                          {%- if block.settings.use_custom_percentage -%}
                            {%- assign percentage_formatted = block.settings.custom_percentage | append: '%' -%}
                          {%- else -%}
                            {% assign percentage_formatted = plan.selling_plans.first.price_adjustments.first.value
                              | ceil
                              | append: '%'
                            %}
                          {%- endif -%}
                          {{ 'products.product.price.save_money_percent' | t: money: percentage_formatted }}
                        </div>
                      {%- endunless -%}
                      <div class="product-information__plan-name">
                        <span class="plan-name__heading">{{ plan_name -}}</span>
                        {%- if block.settings.extra_styles -%}
                          <div class="product-information__price-badge">
                            {%- if block.settings.use_custom_percentage -%}
                              {%- assign percentage_formatted = block.settings.custom_percentage | append: '%' -%}
                            {%- else -%}
                              {% assign percentage_formatted = plan.selling_plans.first.price_adjustments.first.value
                                | ceil
                                | append: '%'
                              %}
                            {%- endif -%}
                            {{ 'products.product.price.save_money_percent' | t: money: percentage_formatted }}
                          </div>
                        {%- endif -%}
                        {%- unless block.settings.plan_sale_description == blank -%}
