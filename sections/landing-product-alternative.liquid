                    data-section="{{ section.id }}"
                  >
                    <button
                      class="buy-quantity__button quantity__button buy-quantity__button-minus no-js-hidden"
                      name="minus"
                      type="button"
                    >
                      {%- render 'buy-buttons-icon-minus' -%}
                    </button>
                    <label class="buy-quantity__input-wrapper">
                      <input
                        class="buy-quantity__input quantity__input "
                        type="number"
                        name="quantity"
                        min="1"
                        id="Quantity-{{ section.id }}"
                        value="{{ product.selected_or_first_available_variant.quantity_rule.min }}"
                        form="{{ product_form_id }}"
                      >
                      {% assign buy_buttons_block = section.blocks | where: "type", "buy_buttons" | first %}
