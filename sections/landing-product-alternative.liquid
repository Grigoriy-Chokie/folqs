                            if block.settings.use_custom_percentage
                              assign sale_percentage = block.settings.custom_percentage
                            else
                              assign sale_percentage = plan.selling_plans.first.price_adjustments.first.value
                            endif
                          %}
                          {% assign compare_at_price_difference = price_compare
                            | divided_by: 100.0
                            | times: sale_percentage
                          %}
                          {% liquid 
                            assign saved_compare_price = price_compare | minus: compare_at_price_difference
                            assign saved_money = price_compare | minus: saved_compare_price
                          %}
                        
                          <p class="product-information__info-text">{{ 'products.product.landing.save_text' | t }} {% if block.settings.widget_you_save_price != blank %} {{ block.settings.widget_you_save_price }} {% else %} {{ saved_money | money }} {% endif %}</p>
                        </div>
                      </div>
                    {% endif %}
                    <label for="{{ input_id }}" class="product-information__plan-label">
                      {% render 'icon-radio' %}
                      {%- unless block.settings.extra_styles -%}
