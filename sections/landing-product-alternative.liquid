                            {{ block.settings.plan_sale_description }}
                          </span>
                        {%- endunless -%}
                        <span class="plan-name__subheading">
                          {% assign subscription_line_pairs = product.metafields.custom.subscription_plan_description
                            | split: '|||'
                          %}
                          {% for line in subscription_line_pairs %}
                            {% assign subscription_line_divided = line | split: '///' %}
                            {% if subscription_line_divided[0] contains plan_name %}
                              {{- subscription_line_divided[1] }}
                            {% endif %}
                          {% endfor %}
                        </span>
                        <div class="product-information__price-text product-information__price-text--mobile">
                          {%- for line in saving_line_pairs -%}
                            {%- assign saving_line_divided = line | split: '///' -%}
                            {%- if saving_line_divided[0] contains plan_name -%}
                              {{- saving_line_divided[1] -}}
                            {%- endif -%}
                          {%- endfor -%}
                        </div>
                        {% if block.settings.additional_plan_subtext != blank %}
                          <p class="product-information__plan-name__additional-subtext">{{ block.settings.additional_plan_subtext }}</p>
                        {% endif %}
                      </div>
                      <div class="product-information__price">
                        
                      {% if block.settings.custom_widget_price != false and block.settings.custom_widget_text != blank %}
                        <p class="product-information__widget-text">{{ block.settings.custom_widget_text }}</p>
                      {% else %}
                        <div class="product-information__price-regular">
                          {% liquid
                            if block.settings.use_custom_percentage
                              assign sale_percentage = block.settings.custom_percentage
                            else
                              assign sale_percentage = plan.selling_plans.first.price_adjustments.first.value
                            endif
                          %}
                          {% assign compare_at_price_difference = price_compare
                            | divided_by: 100.0
                            | times: sale_percentage
                          %}
                          {% if block.settings.widget_regular_price != blank %} {{ block.settings.widget_regular_price }} {% else %} {{ price_compare | minus: compare_at_price_difference | money }} {% endif %}
                        </div>
                        {% if price_compare != blank %}
                          <div class="product-information__price-compare">
                            {% if block.settings.widget_compare_price != blank %} {{ block.settings.widget_compare_price }} {% else %} {{ price_compare | money }} {% endif %}
                          </div>
                        {% endif %}
                        {% if section.settings.show_saving_price == true %}
                          <div class="product-information__price-text">
                            {%- for line in saving_line_pairs -%}
                              {%- assign saving_line_divided = line | split: '///' -%}
                              {%- if saving_line_divided[0] contains plan_name -%}
                                {{- saving_line_divided[1] -}}
                              {%- endif -%}
                            {%- endfor -%}
                          </div>
                        {% endif %}
                      {% endif %}

                        {% if block.settings.use_quantity_box == true %}
                          {% if block.settings.quantity_visible == 'all' or block.settings.quantity_visible == 'subscription' %}
                            <div class="quantity__wrapper{% if block.settings.add_quantity_styles %} quantity__wrapper--styled{% endif %}">
                              <quantity-input
                                class="quantity"
                                data-url="{{ product.url }}"
                                data-section="{{ section.id }}"
                              >
                                <button class="quantity__button no-js-hidden" name="minus" type="button">
                                  {%- if block.settings.add_quantity_styles -%}
                                    {%- render 'icon-quantity-picker', type: 'minus' -%}
                                  {%- else -%}
                                    {% render 'icon-minus' %}
                                  {%- endif -%}
                                </button>
                                <input
                                  class="quantity__input"
                                  type="number"
                                  name="quantity"
                                  min="1"
                                  id="Quantity-{{ section.id }}"
                                  value="{{ product.selected_or_first_available_variant.quantity_rule.min }}"
                                  form="{{ product_form_id }}"
                                >
                                <button class="quantity__button no-js-hidden" name="plus" type="button">
                                  {%- if block.settings.add_quantity_styles -%}
                                    {%- render 'icon-quantity-picker', type: 'plus' -%}
                                  {%- else -%}
                                    {% render 'icon-plus' %}
                                  {%- endif -%}
                                </button>
                              </quantity-input>
                            </div>
                          {% endif %}
                        {% endif %}
                      </div>
                    </label>
                    {% if block.settings.separator != false %}
                      <span class="product-information__separator"> </span>
                    {% endif %}
                    {%- assign plan_name_downcase = plan_name | downcase -%}
                    {% if information_list.size != 0 and name_plan == plan_name_downcase %}
                      <label for="{{ input_id }}">
                        <ul class="plan-name__list {% if block.settings.remove_padding != false %}padding{% endif %}" role="list">
                          {% for information_item in information_list %}
                            <li class="plan-name__list-item">
                              {% if block.settings.icon_style == 'alternative' %}
                                 {{ 'icon-secondary-checkmark-circle.svg' | inline_asset_content }}
                              {% else %}
                                {%- if template.name == 'index' -%}
                                  {{ 'icon-checkmark-circle.svg' | inline_asset_content }}
                                {%- else -%}
                                  {% render 'icon-checkmark' %}
                                {%- endif -%}
                              {% endif %}
                              <span>{{ information_item }}</span>
                            </li>
                          {% endfor %}
                          {% if block.settings.show_popup != blank %}
                            <li class="plan-name__list-item">
                               {% if block.settings.icon_style == 'alternative' %}
                                 {{ 'icon-secondary-checkmark-circle.svg' | inline_asset_content }}
                              {% else %}
                                {%- if template.name == 'index' -%}
                                  {{ 'icon-checkmark-circle.svg' | inline_asset_content }}
                                {%- else -%}
                                  {% render 'icon-checkmark' %}
                                {%- endif -%}
                              {% endif %}
                              {% assign information_list_popup = block.settings.information_popup | newline_to_br | split: '<br />' | default: information_list %}
                              {% render 'popup', block:block, information_list: information_list_popup %}
                              {{- 'cta-banner-popup.js' | asset_url | script_tag -}}                                
                            </li>
                           {% endif %}
                        </ul>
                      </label>
                    {% endif %}
                    {% if subscription_form_list %}
                      <label for="{{ input_id }}">
                        <ul 
                          class="subscription-list list-unstyled"
                          style="--accent-color: {{ subscription_form_list.settings.accent_color }}"
                        >
                          {% for i in (1..6) %}
                            {% liquid 
                              assign text_id = 'text_' | append : i
                              assign text = subscription_form_list.settings[text_id]
                              assign price_text_id = 'text_price_' | append: i
                              assign price_text = subscription_form_list.settings[price_text_id]
                            %}
                            {% if text != blank %}
                              <li class="subscription-list__item">
                                <span class="subscription-list__item-span">
                                  {{ 'icon-secondary-checkmark-circle.svg' | inline_asset_content }}
                                  <p class="subscription-list__text">{{ text }}</p>
                                </span>
                                <p class="subscription-list__price">{{ price_text }}</p>
                              </li>
                            {% endif %}
                          {% endfor %}
                        </ul>

                        {% if subscription_form_list.settings.subscription_total_text != blank %}
                          <div class="subscription-total-text__container">
                            <p class="subscription-total-text">{{ subscription_form_list.settings.subscription_total_text }}</p>
                            <p class=subscription-list__total-price>{{ subscription_form_list.settings.subscription_total_price }}</p>
                          </div>
                        {% endif %}
                      </label>
                    {% endif %}
                    {% if block.settings.gift_info != false and buy_buttons_block.settings.gift_product and buy_buttons_block.settings.gift_product != blank %}
                      <div 
                        class="product-gift__info-block"
                        style="--background: {{ block.settings.gift_item_background }};--text: {{ block.settings.gift_text_color }};--price: {{ block.settings.gift_price_color }}"
                        >
                        <ul class="product-gift__info-list list-unstyled">
                          <li class="product-gift__info-item">
                            {% if block.settings.icon != blank %}
                              <div class="product-gift__item-icon">
                                {{ block.settings.icon | image_url: width: 1000 | image_tag: alt: block.settings.shipping_icon.alt  }}
                              </div>
                            {% endif %}
                            <div class="product-gift__container">
                              <div class="product-gift__text-info">
                                <h2 class="product-gift__title">{{ 'products.product.landing.gift_shipping_text' | t }}</h2>
                                <p class="product-gift__price">{{ 'products.product.landing.gift_shipping_price' | t | money }}</p>
                              </div>
                              {% if block.settings.shipping_icon != blank %}
                                <div class="product-gift__shipping-icon">
                                  {{ block.settings.shipping_icon | image_url: width: 1000 | image_tag: alt: block.settings.shipping_icon.alt }}
                                </div>
                              {% endif %}
                            </div>
                          </li>
                           <li class="product-gift__info-item">
                            {% if block.settings.icon != blank %}
                              <div class="product-gift__item-icon">
                                {{ block.settings.icon | image_url: width: 1000 | image_tag: alt: block.settings.icon.alt }}
                              </div>
                            {% endif %}
                            <div class="product-gift__container">
                              <div class="product-gift__text-info">
                                <h2 class="product-gift__title">{{ block.settings.info_title | default: buy_buttons_block.settings.gift_product.title }}</h2>
                          <p class="product-gift__price">{% if block.settings.product_price != blank %}{{ block.settings.product_price | money }}{% else %}{{ buy_buttons_block.settings.gift_product.price | money }}{% endif %}</p>
                              </div>
                                <div class="product-gift__icon">
                                  {{ block.settings.product_image | default: buy_buttons_block.settings.gift_product.featured_image |  image_url: width: 1000 | image_tag }}
                                </div>
                            </div>                      
                          </li>
                        </ul>
                      </div>
                    {% endif %}
                  </label>
                {% endfor %}
                {% if enable_one_purchase == true %}
                  <label 
                    class="product-information__selling-plan-wrapper {% if block.settings.enable_one_purchase_css != false %}one-purchase-extra-styles{% endif %} product-information__selling-plan-wrapper--one-time-purchase"
                    {% if block.settings.enable_one_purchase_css != false %}
                    style="--border: {{ block.settings.enable_one_purchase_border }}; --text: {{ block.settings.enable_one_purchase_text }}"
                    {% endif %} 
                    >
                    {% assign product_form_id = 'product-form-' | append: section.id %}

                    {%- capture input_id -%}
                      {{ section.id }}-{{ product.selling_plan_groups.size -}}
                    {%- endcapture -%}

                    <input
                      type="radio"
                      id="{{ input_id }}"
                      {% if heading != blank %}
                        name="{{- heading -}}"
                      {% endif %}
                      value=""
                      form="{{ product_form_id }}"
                      class="product-information__radio"
                      data-price="{{ product.price | money_without_currency }}"
                    >
                    <label for="{{ input_id }}" class="product-information__plan-label">
                      {% render 'icon-radio' %}
                      <div class="product-information__plan-name">
                        {% if heading != blank %}
                          <span class="plan-name__heading">{{- heading -}}</span>
                        {% endif %}
                      </div>
                      <div class="product-information__price">
                        <div class="product-information__price-regular">
                          {{ product.price | money }}
                        </div>
                        {% if block.settings.use_quantity_box == true %}
                          {% if block.settings.quantity_visible == 'all' or block.settings.quantity_visible == 'one-time' %}
                            <div class="quantity__wrapper{% if block.settings.add_quantity_styles %} quantity__wrapper--styled{% endif %}">
                              <quantity-input
                                class="quantity"
                                data-url="{{ product.url }}"
                                data-section="{{ section.id }}"
                              >
                                <button class="quantity__button no-js-hidden" name="minus" type="button">
                                  {%- if block.settings.add_quantity_styles -%}
                                    {%- render 'icon-quantity-picker', type: 'minus' -%}
                                  {%- else -%}
                                    {% render 'icon-minus' %}
                                  {%- endif -%}
                                </button>
                                <input
                                  class="quantity__input"
                                  type="number"
                                  name="quantity"
                                  min="1"
                                  id="Quantity-{{ section.id }}"
                                  value="{{ product.selected_or_first_available_variant.quantity_rule.min }}"
                                  form="{{ product_form_id }}"
                                  {% if product.selling_plan_groups.size > 0 %}
                                    disabled
                                  {% endif %}
                                >
                                <button class="quantity__button no-js-hidden" name="plus" type="button">
                                  {%- if block.settings.add_quantity_styles -%}
                                    {%- render 'icon-quantity-picker', type: 'plus' -%}
                                  {%- else -%}
                                    {% render 'icon-plus' %}
                                  {%- endif -%}
                                </button>
                              </quantity-input>
                            </div>
                          {% endif %}
                        {% endif %}
                      </div>
                    </label>
                  </label>
                {% endif %}
              </div>
            {%- when 'one-time-packs' -%}
              {%- liquid
                assign packs = block.settings.packs | split: ','
                assign preselected_item = block.settings.preselected_item
              -%}

              {% for pack in packs %}
                {% liquid
                  assign pack = pack | times: 1
                  assign pack_index = forloop.index
                  assign pack_label = pack | times: 1 | pluralize: 'Pack', 'Packs'
                  assign input_id = product.handle | append: '_quantity_' | append: pack

                  assign discount_code_key = 'pack_' | append: pack_index | append: '_discount_code'
                  assign discount_percent_key = 'pack_' | append: pack_index | append: '_discount_percent'
                  assign discounted_price_key = 'pack_' | append: pack_index | append: '_discounted_price'
                  assign pack_title_key = 'pack_' | append: pack_index | append: '_pack_title'
                  assign pack_text_key = 'pack_' | append: pack_index | append: '_pack_text'
                  assign pack_servings_key = 'pack_' | append: pack_index | append: '_pack_servings'

                  assign discount_code = block.settings[discount_code_key]
                  assign discount_percent = block.settings[discount_percent_key] | ceil | append: '%'
                  assign discounted_price = block.settings[discounted_price_key] | times: 100
                  assign pack_title = block.settings[pack_title_key]
                  assign pack_text = block.settings[pack_text_key]
                  assign pack_servings = block.settings[pack_servings_key]

                  assign subtotal_discounted_price = discounted_price | times: pack | money
                  assign subtotal_compare_at_price = product.price | times: pack | money
                  assign product_form_id = 'product-form-' | append: section.id
                %}

                <label class="product-information__selling-plan-wrapper product-information__one-time-pack-wrapper {% if forloop.index == preselected_item %}active {% endif %}">
                  <input
                    type="radio"
                    id="{{ input_id }}"
                    name="quantity"
                    value="{{ pack }}"
                    form="{{ product_form_id }}"
                    class="product-information__radio product-information__radio--one-time-quantity"
                    data-discount-code="{{- discount_code | default: '' -}}"
                    {% if forloop.index == preselected_item %}
                      checked
                    {% endif %}
                  >
                  <label for="{{ input_id }}" class="product-information__plan-label">
                    {% render 'icon-radio' %}
                    <div class="product-information__price-badge">
                      {{ 'products.product.price.save_money_percent' | t: money: discount_percent }}
                    </div>
                    <div class="product-information__plan-name">
                      <span class="plan-name__heading">{{- pack_title -}}</span>
                      <span class="plan-name__subheading">{{- pack_text -}}</span>
                      <div class="product-information__price-text product-information__price-text--mobile">
                        {{- pack_servings -}}
                      </div>
                    </div>
                    <div class="product-information__price">
                      <div class="product-information__price-regular">
                        {{- subtotal_discounted_price -}}
                      </div>
                      {% if subtotal_compare_at_price != blank %}
                        <div class="product-information__price-compare">
                          {{- subtotal_compare_at_price -}}
                        </div>
                      {% endif %}
                      <div class="product-information__price-text">
                        {{- pack_servings -}}
                      </div>
                    </div>
                  </label>
                </label>
              {% endfor %}

