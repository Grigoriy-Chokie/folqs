{{ 'section-landing-page.css' | asset_url | stylesheet_tag }}
{{ 'section-landing-page-alternative.css' | asset_url | stylesheet_tag }}
{{ 'component-faq.css' | asset_url | stylesheet_tag }}
{{ 'section-landing-product-colostrum-fabula.css' | asset_url | stylesheet_tag }}
{{ 'component-lightbox.css' | asset_url | stylesheet_tag }}

{% if section.settings.enable_style == true %}
  {{ 'section-landing-page-alternative-extra.css' | asset_url | stylesheet_tag }}
{% endif %}

{% if template.suffix == 'bovine-landing-folqs-gift' %}
  {{ 'stamped-main-widget.css' | asset_url | stylesheet_tag }}
{% endif %}

{% if section.settings.use_gallery == true -%}
  {{ 'section-landing-product-offer-v2.css' | asset_url | stylesheet_tag }}
{% endif %}

{% if section.settings.enable_extra_styles == true %}
  {{ 'landing-page-extra-styles.css' | asset_url | stylesheet_tag }}
{% endif %}

{% if section.settings.zoom_type == 'default' %}
  <script src="{{ 'component-magnify-zoom.js' | asset_url }}" defer="defer"></script>
{% endif %}
{% if section.settings.zoom_type == 'magnify' %}
  <script src="{{ 'component-magnify.js' | asset_url }}" defer="defer"></script>
{% endif %}
<script src="{{ 'component-lightbox.js' | asset_url }}" defer="defer"></script>

{%- liquid
  assign additional_style_files = section.settings.additional_style_files | newline_to_br | split: '<br />'
  assign additional_style_classes = ''
  assign icon_text_block_count = 0
  assign icon_text_slider_block_count = 0

  if additional_style_files != blank
    for file in additional_style_files
      echo file | asset_url | stylesheet_tag
      assign file_class = file | replace: '.css', ''
      assign additional_style_classes = additional_style_classes | append: file_class | append: ' '
    endfor
  endif
-%}

{% for block in section.blocks %}
  {% if block.type == 'icon-text' and block.settings.text != blank %}
    {% assign icon_text_block_count = icon_text_block_count | plus: 1 %}
  {% endif %}

  {% if block.type == 'icon-text-slider' and block.settings.text != blank %}
    {% assign icon_text_slider_block_count = icon_text_slider_block_count | plus: 1 %}
  {% endif %}
{% endfor %}

{% style %}
.landing-product__mobile-blocks {
  display: none;
}
  
  .section-{{ section.id }}--padding {
    position: relative;
    background: {{ section.settings.background }};
    padding-top: {{ section.settings.padding_top_mobile }}px;
    padding-bottom: {{ section.settings.padding_bottom_mobile }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}--padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{% endstyle %}

{%- liquid
  assign product_images = section.blocks | where: 'type', 'image'
  assign style_type = section.settings.style_type
  assign gallery_full_width = section.settings.gallery_full_width
  assign center_thumbs = section.settings.center_thumbs
  assign product = section.settings.landing_product | default: product
  assign product_form_id = 'form_id-' | append: product.id
  assign variant = product.selected_or_first_available_variant
  assign price = variant.price
  assign price_compare = variant.compare_at_price
  assign has_features = false
  if variant.selling_plan_allocations != blank
    assign price = variant.selling_plan_allocations[0].price
    assign selling_plan = variant.selling_plan_allocations[0].selling_plan.id
    if price_compare == blank
      assign price_compare = variant.price
    endif
  endif
  assign is_one_time_packs = section.blocks | where: 'type', 'one-time-packs'

  assign primary_color = section.settings.primary_color | default: '#ff6a13'
  assign collapsible_row_blocks = section.blocks | where: 'type', 'collapsible_row'
  assign scrolling_id = 'landing-product-' | append: product.id
-%}

{%- capture sizes -%}
  (min-width: {{ settings.page_width }}px) 645px, (min-width: 990px) calc(100.0vw - 10rem), (min-width: 750px) 100vw
{%- endcapture -%}

{% if section.settings.enable_extra_styles_pdp %}
  <style>
    @media screen  and (max-width: 749px) {
      .icon-text-slider-blocks {
        width: calc(100% - 10px);
        margin-left: 5px;
      }

    }

    @media screen and (max-width: 750px) {
      .landing-product__mobile-blocks {
        display: none !important;
      }

      .product-information__reviews.product-block.product-block--dektop {
        display: flex !important;
      }
    }
  </style>
{% endif %}


{% if product != blank %}
  <div
    class="
      landing-product section-{{ section.id }}--padding
      {% if section.settings.use_gallery == true %}
      landing-product--gallery
      {% endif %}
      landing-product--height-{{ style_type }}
      {{ additional_style_classes }}
      {% if section.settings.enable_extra_styles == true %}is--extra-styles{% endif %}
    "
    style="--primary-color: {{ primary_color }}; --primary-color-rgb: {{ primary_color.rgb }};"
    id="landing-product"
    {% if request.page_type == 'product' and product.metafields.custom.redirect_link != blank %}
    data-redirect-to="{{ product.metafields.custom.redirect_link }}"
    {% endif %}
  >

    <div class="landing-product__container page-width landing-product__container--{{ section.settings.mobile_style }}">
      {% if section.settings.background_icon != blank %}
        <div class="landing-product__decor">
          {{ section.settings.background_icon }}
        </div>
      {% endif %}

      {% if style_type == 'compact' %}
        <div id="landing-product-{{ product.id }}--mobile" class="landing-product__information--mobile">
          {% for block in section.blocks %}
            {% case block.type %}
              {%- when 'subtitle' -%}
                <p class="landing-product__subtitle">{{- block.settings.subtitle -}}</p>
              {%- when 'title' -%}
                <div class="landing-product__title product-block">
                  {% if block.settings.text_mobile != blank %}
                    <div class="product-block__title--mobile">
                      {{ block.settings.text_mobile }}
                    </div>
                  {% endif %}
                  <h2
                    id="landing-product-alternative"
                    class="product-block__title"
                    style="--accent-color: {{ block.settings.highlighted_color }}"
                  >
                    {%- if block.settings.text != blank -%}
                      {{- block.settings.text -}}
                    {%- else -%}
                      {{- product.title -}}
                    {%- endif -%}
                  </h2>
                </div>

              {%- when 'reviews' -%}
                {% liquid
                  assign tag = 'div'
                  if block.settings.url != blank
                    assign tag = 'a'
                  endif
                %}

                <{{ tag }}
                  href="{{ block.settings.url }}"
                  class="product-information__reviews product-block"
                  id="product-information__reviews--{{ forloop.index }}--mobile"
                >
                  <span class="product-information__reviews-icon">
                    {%- case block.settings.stars_type -%}
                      {%- when 'yellow' -%}
                        {%- render 'icon-stars' -%}
                      {%- when 'orange' -%}
                        {{ 'icon-stars-new.svg' | inline_asset_content }}
                      {%- when 'orange_small' -%}
                        {%- render 'icon-rating-extra' -%}
                    {%- endcase -%}
                  </span>
                  {% if block.settings.text != blank %}
                    <span class="product-information__reviews-text">
                    {{- block.settings.text -}}</span>
                  {% endif %}
                </{{ tag }}>
            {% endcase %}
          {% endfor %}
          {%- if collapsible_row_blocks.size > 0 -%}
            <div class="faq__content faq__content--{{ section.settings.faq_style }}">
              {% for block in collapsible_row_blocks %}
                {%- liquid
                  assign question = block.settings.question
                  assign answer = block.settings.answer
                  assign has_content = false
                  if question != blank and answer != blank
                    assign has_content = true
                  endif

                  if has_content == true
                    render 'collapse-row', icon_style: 'new', title: question, content: answer, class_name_row: 'faq__content-item faq-item', class_name_title: 'faq-item__question', class_name_content: 'faq-item__answer', is_first_opened: is_first_opened, index: forloop.index0
                  endif
                -%}
              {% endfor %}
            </div>
          {%- endif -%}
        </div>
      {% endif %}
      <div class="landing-product__mobile-blocks">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'reviews' -%}
              {% liquid
                assign tag = 'div'
                if block.settings.url != blank
                  assign tag = 'a'
                endif
              %}
              <{{ tag }}
                href="{{ block.settings.url }}"
                class="product-information__reviews product-block"
              >
                <span class="product-information__reviews-icon">
                  {%- case block.settings.stars_type -%}
                    {%- when 'yellow' -%}
                      {%- render 'icon-stars' -%}
                    {%- when 'orange' -%}
                      {{ 'icon-stars-new.svg' | inline_asset_content }}
                    {%- when 'orange_small' -%}
                      {%- render 'icon-rating-extra' -%}
                  {%- endcase -%}
                </span>
                {% if block.settings.text != blank %}
                  <span class="product-information__reviews-text">{{- block.settings.text -}}</span>
                {% endif %}
              </{{ tag }}>
            {%- when 'title' -%}
              <div class="landing-product__title product-block">
                {% if block.settings.text_mobile != blank %}
                  <div class="product-block__title--mobile">
                    {{ block.settings.text_mobile }}
                  </div>
                {% endif %}
                <h2
                  id="landing-product-alternative"
                  class="product-block__title"
                  style="--accent-color: {{ block.settings.highlighted_color }}"
                >
                  {%- if block.settings.text != blank -%}
                    {{- block.settings.text -}}
                  {%- else -%}
                    {{- product.title -}}
                  {%- endif -%}
                </h2>
              </div>
          {%- endcase -%}
        {%- endfor -%}
      </div>
      {% if product.featured_image != blank -%}
        {%- capture sizes -%}
          (min-width: {{ settings.page_width }}px) 645px, (min-width: 990px) calc(100.0vw - 10rem), (min-width: 750px) 100vw
        {%- endcapture -%}
        {% if section.settings.use_gallery == true %}
          {% if section.settings.gallery_view == 'slider' %}
            <product-offer-v2-media data-id="{{ section.id }}" class="landing-product-offer-v2__media product-offer-v2-media">
              <div class="product-offer-v2-media__main offer-v2-media-main">
                <div class="offer-v2-media-main__swiper-wrapper swiper-wrapper">
                  {%- for image_block in product_images -%}
                    <div
                      class="offer-v2-media-main__slide product__modal-magnify swiper-slide"
                      data-image="{{ image_block.settings.image | image_url: width: 2000  }}"
                    >
                      {%- if image_block.settings.image != blank -%}
                          {% assign image_classes = 'offer-v2-media-main__image ' | append: 'image-magnify-' | append: section.settings.zoom_type %}
                        {{
                          image_block.settings.image
                          | image_url: width: 1946
                          | image_tag:
                            loading: 'lazy',
                            sizes: sizes,
                            widths: '246, 493, 600, 713, 823, 990, 1100, 1206, 1346, 1426, 1646, 1946',
                            class: image_classes
                        }}
                      {%- else -%}
                        {{ 'image' | placeholder_svg_tag }}
                      {%- endif -%}
                    </div>
                  {%- endfor -%}
                </div>

                {%- if section.settings.is_show_pagination -%}
                  <div class="offer-v2-media-main__pagination--bullets swiper-pagination"></div>
                {%- endif -%}
                {%- if section.settings.is_show_pagination_fraction -%}
                  <div class="offer-v2-media-main__pagination--fraction">1/5</div>
                {%- endif -%}
                {% if section.settings.show_mobile_zoom_icon != false %}
                  <div class="product-offer-v2-media__zoom-icon">
                    {{ 'zoom-icon.svg' | inline_asset_content }}
                  </div>
                {% endif %}

                <div class="product-offer-v2-media__navigation product-offer-v2-media__navigation--prev">
                  <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none">
                    <path d="M8.80224 1.94249C8.91307 1.94249 9.02391 1.98332 9.11141 2.07082C9.28057 2.23999 9.28057 2.51999 9.11141 2.68916L5.30807 6.49249C5.02807 6.77249 5.02807 7.22749 5.30807 7.50749L9.11141 11.3108C9.28057 11.48 9.28057 11.76 9.11141 11.9292C8.94224 12.0983 8.66224 12.0983 8.49307 11.9292L4.68974 8.12582C4.39224 7.82832 4.22307 7.42582 4.22307 6.99999C4.22307 6.57415 4.38641 6.17165 4.68974 5.87415L8.49307 2.07082C8.58057 1.98916 8.69141 1.94249 8.80224 1.94249Z" fill="#292D32"/>
                  </svg>
                </div>
                <div class="product-offer-v2-media__navigation product-offer-v2-media__navigation--next">
                  <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none">
                    <path d="M5.19776 12.0575C5.08693 12.0575 4.97609 12.0167 4.88859 11.9292C4.71943 11.76 4.71943 11.48 4.88859 11.3108L8.69193 7.50751C8.97193 7.22751 8.97193 6.77251 8.69193 6.49251L4.88859 2.68918C4.71943 2.52001 4.71943 2.24001 4.88859 2.07084C5.05776 1.90168 5.33776 1.90168 5.50693 2.07084L9.31026 5.87418C9.60776 6.17168 9.77693 6.57418 9.77693 7.00001C9.77693 7.42584 9.61359 7.82834 9.31026 8.12584L5.50693 11.9292C5.41943 12.0108 5.30859 12.0575 5.19776 12.0575Z" fill="#292D32"/>
                  </svg>
                </div>

                <div class="product-offer-v2-media__badges-container">
                  {%- for option in one_time_options -%}
                    {% assign style_display = 'none' %}
                    {% if forloop.index0 == 1 %}
                      {% assign style_display = 'block' %}
                    {% endif %}
                    {%- assign media_badges = option.settings.image_badges | newline_to_br | split: '<br />' -%}
                    {%- for badge in media_badges -%}
                      <div
                        class="product-offer-v2-media__badge"
                        data-badge-id="badge-one-time-{{ option.settings.quantity }}"
                        style="display: {{ style_display }}; background-color: rgba(var(--active-color), .1); color: rgb(var(--active-color));"
                      >
                        {{- badge -}}
                      </div>
                    {%- endfor -%}
                  {%- endfor -%}
                  {%- for option in subscribe_options -%}
                    {% assign style_display = 'none' %}
                    {%- assign media_badges = option.settings.image_badges | newline_to_br | split: '<br />' -%}
                    {%- for badge in media_badges -%}
                      <div
                        class="product-offer-v2-media__badge"
                        data-badge-id="badge-subscribe-{{ option.settings.quantity }}"
                        style="display: {{ style_display }}; background-color: rgba(var(--active-color), .4); color: rgb(var(--active-color));"
                      >
                        {{- badge -}}
                      </div>
                    {%- endfor -%}
                  {%- endfor -%}
                </div>
              </div>
              <div class="product-offer-v2-media__thumbs offer-v2-media-thumbs">
                <div class="offer-v2-media-thumbs__swiper-wrapper swiper-wrapper {% if center_thumbs != false %}offer-v2-media-thumbs__swiper-wrapper--center{% endif %}">
                  {%- for image_block in product_images -%}
                    <div class="offer-v2-media-thumbs__slide swiper-slide">
                      {%- if image_block.settings.image != blank -%}
                        {{ image_block.settings.image | image_url: width: 300 | image_tag }}
                      {%- else -%}
                        {{ 'image' | placeholder_svg_tag }}
                      {%- endif -%}
                    </div>
                  {%- endfor -%}
                </div>
              </div>
              {% render 'lightbox', section: section, product_images: product_images %}
              {% if icon_text_slider_block_count > 0 %}
                <div class="icon-text-slider-blocks">
                  {% for block in section.blocks %}
                    {% if block.type == 'icon-text-slider' and block.settings.text != blank %}
                      <div class="icon-text-slider-block">
                        {% if block.settings.icon != blank %}
                          <img src="{{ block.settings.icon | image_url: width: 18, height: 18 }}" alt="{{ block.settings.icon.alt }}" width="18" height="18" loading="lazy">
                        {% endif %}
                        <span>{{ block.settings.text | newline_to_br }}</span>
                      </div>
                    {% endif %}
                  {% endfor %}
                </div>
              {% endif %}
            </product-offer-v2-media>
          {%- elsif section.settings.gallery_view == 'grid' -%}
            <div class="landing-product-offer-v2__media product-offer-v2-media">
              <div class="product-offer-v2-media__main offer-v2-media-main">
                <div class="offer-v2-media-main__grid-wrapper">
                  {%- for image_block in product_images -%}
                    <div class="offer-v2-media-main__grid-item offer-v2-media-main__slide">
                      {%- if image_block.settings.image != blank -%}
                        {{
                          image_block.settings.image
                          | image_url: width: 1946
                          | image_tag:
                            loading: 'lazy',
                            sizes: sizes,
                            widths: '246, 493, 600, 713, 823, 990, 1100, 1206, 1346, 1426, 1646, 1946'
                        }}
                      {%- else -%}
                        {{ 'image' | placeholder_svg_tag }}
                      {%- endif -%}
                    </div>
                  {%- endfor -%}
                </div>
              </div>
            </div>
          {% endif %}
        {% else %}
          <div class="landing-product__item landing-product__image">
            <div
              class="
                landing-product__image-wrapper {% if gallery_full_width == true %}
                  landing-product__image-wrapper--full-width
                {% endif %}
              "
            >
              {{
                product.featured_image
                | image_url: width: master
                | image_tag: widths: '900', alt: product.featured_image.alt, loading: 'lazy'
              }}
            </div>
          </div>
        {% endif %}
      {% endif %}
    <div id="landing-product-{{ product.id }}" class="landing-product__item landing-product__information {% if template.suffix == 'bovine-landing-fabula' %}scroll-margin-block{% endif %}" {% if request.page_type == 'index' %}style="scroll-margin-top: 10px"{% endif %}>
        {% for block in section.blocks %}
          {%- case block.type -%}
            {%- when 'subtitle' -%}
              <p class="landing-product__subtitle">{{- block.settings.subtitle -}}</p>
            {%- when 'title' -%}
              <div id="landing-product__info-block" class="landing-product__title product-block product-block--dektop">
                {% if block.settings.text_mobile != blank %}
                  <div class="product-block__title--mobile">
                    {{ block.settings.text_mobile }}
                  </div>
                {% endif %}
                <h2
                  id="landing-product-alternative"
                  class="product-block__title"
                  style="--accent-color: {{ block.settings.highlighted_color }}"
                >
                  {%- if block.settings.text != blank -%}
                    {{- block.settings.text -}}
                  {%- else -%}
                    {{- product.title -}}
                  {%- endif -%}
                </h2>
              </div>
            {%- when 'rating' -%}
              <div class="product__rating-block">
                {%- unless block.settings.rating_image == blank -%}
                  <div class="product__rating-image">
                    {{ block.settings.rating_image | image_url: width: 1000 | image_tag }}
                  </div>
                {%- endunless -%}
                <div class="rating-block__stars">{%- render 'icon-rating-extra' -%}</div>
                {%- unless block.settings.rating_text == blank -%}
                  <p class="product__rating-text product__rating-text--desktop">{{ block.settings.rating_text }}</p>
                  <p class="product__rating-text product__rating-text--mobile">{{ block.settings.rating_text_mobile }}</p>
                {%- endunless -%}
              </div>
            {%- when 'reviews' -%}
              {% liquid
                assign tag = 'div'
                if block.settings.url != blank
                  assign tag = 'a'
                endif
              %}
              <{{ tag }}
                href="{{ block.settings.url }}"
            class="product-information__reviews product-block product-block--dektop {% if block.settings.show_desktop_position != false%}show-desktop-position{% endif %}"
                id="product-information__reviews--2"
              >
                <span class="product-information__reviews-icon">
                  {%- case block.settings.stars_type -%}
                    {%- when 'yellow' -%}
                      {%- render 'icon-stars' -%}
                    {%- when 'orange' -%}
                      {{ 'icon-stars-new.svg' | inline_asset_content }}
                    {%- when 'orange_small' -%}
                      {%- render 'icon-rating-extra' -%}
                  {%- endcase -%}
                </span>
                {% if block.settings.text != blank %}
                  <span class="product-information__reviews-text">{{- block.settings.text -}}</span>
                {% endif %}
                {% if block.settings.additional_text %}
                  <div class="product-information__reviews-additional_text">
                    {{ block.settings.additional_text }}
                  </div>
                {% endif %}
              </{{ tag }}>
            {%- when 'feature' -%}
              {%- unless has_features -%}
                {%- assign features = section.blocks | where: 'type', 'feature' -%}
                {%- if features.size > 0 -%}
                  {%- assign has_features = true -%}
                  <div class="product-information__features product-block">
                    {%- for block in features -%}
                      {% if block.settings.icon and block.settings.title != blank %}
                        <div class="product-information__feature{% if block.settings.hide_on_mobile %} product-information__feature--hide-mobile{% endif %}">
                          <div class="product-information__feature-icon">
                            {{
                              block.settings.icon
                              | image_url: width: master
                              | image_tag: widths: '100', alt: block.settings.icon.alt, loading: 'lazy'
                            }}
                          </div>
                          <div class="product-information__feature-content">
                            <h4>
                              {{ block.settings.title | newline_to_br }}
                            </h4>
                            <h4 class="title--mobile">
                              {{ block.settings.title_mobile | newline_to_br | default: block.settings.title | newline_to_br  }}
                            </h4>
                            {% if block.settings.text != blank %}
                              <p>
                                {{ block.settings.text }}
                              </p>
                            {% endif %}
                          </div>
                        </div>
                      {% endif %}
                    {%- endfor -%}
                  </div>
                {%- endif -%}
              {%- endunless -%}

            {% when 'text_with_icon' %}
              <div class="text-with-icon__container">
                <ul class="text-with-icon__list list-unstyled">
                  {% for i in (1..2) %}
                    <li class="text-with-icon__item">
                      {% liquid 
                        assign icon_id = 'icon_' | append: i
                        assign icon = block.settings[icon_id]
                        assign text_id = 'text_' | append: i
                        assign text = block.settings[text_id]
                        assign mobile_text_id = 'mobile_text_' | append: i
                        assign mobile_text = block.settings[mobile_text_id]
                      %}
                      {% if text != blank %}
                        {% if icon != blank %}
                          <div>
                            {{ icon | image_url: width: icon.width | image_tag: alt: icon.alt }}
                          </div>
                        {% endif %}
                        <div class="text-with-icon__text">
                          <div class="text-with-icon__text--desktop">{{ text }}</div>
                          <div class="text-with-icon__text--mobile">{{ mobile_text }}</div>
                        </div>
                      {% endif %}
                    </li>
                  {% endfor %}
                </ul>
              </div>
            {% when 'price' %}
              {% assign saving_line_pairs = product.metafields.custom.subscription_savings_line | split: '|||' %}

              {%- assign enable_one_purchase = block.settings.enable_one_purchase -%}
              {%- assign heading = block.settings.heading -%}
              {%- assign name_plan = block.settings.name_plan | downcase | strip -%}
              {%- assign information_list = block.settings.information | newline_to_br | split: '<br />' -%}
              {% assign buy_buttons_block = section.blocks | where: "type", "buy_buttons" | first %}
              {% assign subscription_form_list = section.blocks | where: "type", "subscription_form_list" | first %}
              {% capture style_variables %}
                  --primary-color: {{ block.settings.primary_color }};
                  --badge-background: {{ block.settings.badge_background }};
                  --badge-color: {{ block.settings.badge_color }};
                  --shadow-radius: {% if block.settings.shadow == true %}4px{% else %}0px{% endif %};
                  --background-color: {{ block.settings.background_color }};
